<TESTSCONFIGURATION>

<QTEST name="KolmogorovQTest">
        <TYPE>Comp2RefKolmogorov</TYPE>	
	<PARAM name="testparam">0</PARAM>
	<PARAM name="error">0.3</PARAM>
	<PARAM name="warning">0.3</PARAM>
</QTEST>

<QTEST name="EfficienciesAbove0.8">
        <TYPE>ContentsYRange</TYPE>
        <PARAM name="ymin">0.8</PARAM>
        <PARAM name="ymax">1.0</PARAM>
        <PARAM name="error">0.90</PARAM>
        <PARAM name="warning">0.99</PARAM>
</QTEST>

<LINK name="HLT/Muon/Distributions/*/genEff*">
	<TestName activate="true">KolmogorovQTest</TestName>
</LINK>

<LINK name="HLT/Muon/Distributions/*/genEff*L1*">
	<TestName activate="true">EfficienciesAbove0.8</TestName>
</LINK>

<LINK name="HLT/Muon/Distributions/*/genEff*L3*">
	<TestName activate="true">EfficienciesAbove0.8</TestName>
</LINK>

</TESTSCONFIGURATION>
